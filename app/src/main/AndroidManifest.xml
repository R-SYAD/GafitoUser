<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GafitoUser"
        tools:targetApi="31">
        <activity
            android:name=".user.screens.Navigation"
            android:exported="false"
            android:label="@string/title_activity_navigation"
            android:theme="@style/Theme.GafitoUser" />
        <activity
            android:name=".user.component.ShowQR"
            android:exported="false"
            android:label="@string/title_activity_show_qr"
            android:theme="@style/Theme.GafitoUser" />
        <activity
            android:name=".user.component.ShowQRActivity"
            android:exported="false"
            android:label="@string/title_activity_show_qractivity"
            android:theme="@style/Theme.GafitoUser" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GafitoUser">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>